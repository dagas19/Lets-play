<% @events.each do |event| %>
<div class="event-card">
  <div class="main-info text-center">
    <h2 class="eve-title display-5" style="color: rgb(8,146,208);"> <%= event.title %></h2>

    <div class="cont text-center">
      <img src="https://static-s.aa-cdn.net/img/ios/504572536/235293dda92e8fa4ebf4d6d9a41b2bbf?v=1" alt="" class="event-img">
      <p class="font-bold desc"><%= event.description %></p>
      <p class="font-bold desc time"><%= event.date.strftime("Will be happening this %A") %></p>
    </div>
  </div>
  <div class="small-info d-flex justify-content-between align-items-center">
    <div class="sma d-flex">
      <img src="data:image/jpeg;base64," alt="" class="small-avatar">
      <p class="created-by">Created by <%= event.user.nickname %></p>
    </div>
    <div>

      <%= link_to "Enter the event", event_path(event), class: "btn entry btn-primary btn-lg", style: "-webkit-text-stroke: 1px white;" %>
    </div>
    <div class="loc d-flex">
        <p class="loc-d" ><%= event.venue.name %>  <% if user_signed_in? && current_user.address.present? %> -
          <%= (current_user.distance_to(event.venue.to_coordinates) * 1.35 ).round %> km  away <i class="fas fa-map-marker-alt"></i> <% end  %>  </p>
    </div>
  </div>
</div>
<% end %>
